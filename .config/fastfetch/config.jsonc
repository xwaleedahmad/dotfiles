    //   "type": "localIP",
    //   "key": "├󰩟 Local IP",
    //   "keyColor": "yellow",
    // },
    {
      "type": "uptime",
      "key": "└󰅐 Start Time",
      "keyColor": "yellow",
    },
    "break",
    {
      "type": "wm",
